
(rules PCB esp32c3
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 53)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
  )
  (rule
    (width 250.0)
    (clear 200.0)
    (clear 125.0 (type smd_to_turn_gap))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    (clearance_class "kicad_default")
    (via_rule "kicad_default")
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "ƒNƒ‰ƒX1"
    +3.3V "Net-(J2-Pin_2)" "Net-(J2-Pin_3)" "Net-(J2-Pin_4)" "Net-(J2-Pin_5)" "Net-(J2-Pin_6)" "Net-(D1-A)" "Net-(D2-A)"
    GND "Net-(J3-Pin_1)" "Net-(J3-Pin_2)" "Net-(J1-Pin_7)" "Net-(J1-Pin_6)" "Net-(J1-Pin_5)" "Net-(J1-Pin_4)" "Net-(J1-Pin_3)"
    "Net-(J1-Pin_2)" "Net-(J1-Pin_1)" "Net-(J3-Pin_3)" "Net-(U1-EN{slash}CHIP_PU)" "Net-(U1-GPIO8)"
    (clearance_class default)
    (via_rule default)
    (rule
      (width 750.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)